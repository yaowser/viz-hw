PImage img;

void settings() {           
  img = loadImage("1.jpg");
  int iWidth = img.width*2;
  int iHeight = img.height;
  size(iWidth, iHeight);
}

void mousePressed() {
  if (mouseButton == LEFT) {
    int x = img.width/15;
    int y = img.height/30;
    stroke(1);
    for(int i = 0; i < img.width; i += x){
      for(int j = 0; j < img.height; j += y){
        fill(int(random(255)),int(random(255)),int(random(255)));
        rect(i,j,x,y);
      }
    }
  } else if(mouseButton == RIGHT) {
    save("final.jpg");
  }
}

